@using System.Web.Optimization
@using Blocks.Framework.Web.Mvc.Extensions
@using Blocks.Framework.Web.Mvc.UI.Resources
@{
    Layout = ResourceManager.GetTemplateNotCache("Tradition/_LayoutPartialModule", this.VirtualPath);
    // Style.Include("IndexCss.css").AtHead();
}
<div class="modal-dialog">
    <div class="modal-content">
        <form name="tenantCreateForm" role="form" novalidate class="form-validation">
            <div class="modal-header">
                <h4 class="modal-title">
                    <span>CreateNewTenant</span>
                </h4>
            </div>
            <div class="modal-body">
                <div class="form-group form-float">
                    <div class="form-line">
                        <input class="form-control" type="text" name="TenancyName" required maxlength="10" minlength="2">
                        <label class="form-label">TenancyName</label>
                    </div>
                </div>
                <div class="form-group form-float">
                    <div class="form-line">
                        <input type="text" name="Name" class="form-control" required maxlength="10">
                        <label class="form-label">Name</label>
                    </div>
                </div>
                <div class="form-group form-float">
                    <div class="form-line">
                        <input type="text" name="ConnectionString" class="form-control" maxlength="10">
                        <label class="form-label">Optional</label>
                    </div>
                </div>
                <div class="form-group form-float">
                    <div class="form-line">
                        <input type="email" name="AdminEmailAddress" class="form-control" required maxlength="10">
                        <label class="form-label">AdminEmailAddress</label>
                    </div>
                </div>
                <div class="checkbox">
                    <input type="checkbox" name="IsActive" value="true" id="CreateTenantIsActive" class="filled-in" checked/>
                    <label for="CreateTenantIsActive">IsActive</label>
                </div>
                <p>"password"</p>
            </div>
            <table id="gridInfo"></table>
            <div class="modal-footer">
                <button type="button" class="btn btn-default waves-effect" data-dismiss="modal">Cancel</button>
                <button type="submit" class="btn btn-primary waves-effect">Save</button>
            </div>
        </form>
    </div>
</div>
